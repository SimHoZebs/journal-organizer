{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev": "concurrently \"pnpm run dev:frontend\" \"pnpm --filter @stt/backend run dev\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "pnpm biome lint ./src ./backend",
    "format": "pnpm biome format --write ./src ./backend"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.12",
    "@uiw/react-md-editor": "^4.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "10.1.0",
    "react-router": "^7.4.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.0.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@iconify/json": "^2.2.331",
    "@iconify/tailwind4": "^1.0.6",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/wicg-file-system-access": "^2023.10.6",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "jsdom": "^26.0.0",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.8.3",
    "vite": "^6.1.0",
    "vitest": "^3.1.1"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "keywords": [],
